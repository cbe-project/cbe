	<default-route>
		<any-service> <parent/> <any-child/> </any-service>
	</default-route>

	<default caps="200"/>

	<start name="dynamic_rom">
		<resource name="RAM" quantum="4M"/>
		<provides><service name="ROM"/> </provides>
		<config verbose="no">
			<rom name="runtime.config">

				<inline> <config/> </inline>

				<sleep milliseconds="1000"/>

